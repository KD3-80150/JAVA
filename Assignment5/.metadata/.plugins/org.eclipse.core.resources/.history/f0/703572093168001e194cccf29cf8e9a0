package com.app.fruits;

public class Apple 
{

	int arr1[][] = new int[2][3];

	arr1[0][0] = 10;
	arr1[0][1] = 20;
	arr1[0][2] = 30;
	arr1[1][0] = 40;
	arr1[1][1] = 50;
	arr1[1][2] = 60;

	for (int i = 0; i < 2; i++)
		for (int j = 0; j < 3; j++)
			System.out.println("Element at arr1[" + i + "][" + j + "] = " + arr1[i][j]);
	
}
