package com.app.fruits;

import java.util.Iterator;

public class Mango {

	public static void main(String[] args) {
		int arr1[] = new int[5];
		arr1[0]=10;
		arr1[1]=20;
		arr1[2]=30;
		arr1[3]=40;
		arr1[4]=50;
	
		for (int i = 0; i < 5; i++) 
		{
		System.out.println("Element at "+ i +"= "+arr1[i]);
		}
		
		int arr2[] = new int[] { 11,22,33,44,55};
		for (int i = 0; i < 5; i++) 
		{
		System.out.println("Element at index "+i+"= "+arr2[i]);	
		}
		
		int arr3[] = {110,220,330,440,550};
		for (int num : arr3) 
		{
		System.out.println("Element = " + num);	
		}
	}
}
