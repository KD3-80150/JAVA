		Scanner sc = new Scanner(System.in);
		Map<String, Book5> map = new HashMap<String, Book5>();
		int choice;
		Book5 b;
		do {
			System.out.println("1.insert in map.");
			System.out.println("2.Find in map. ");

			System.out.println("Enter choice = ");
			choice = sc.nextInt();
			switch (choice) {
			case 1:
				b = new Book5();
				b.accept();
				map.put(b.getIsbn(), b);

				break;
			case 2:
				System.out.println("Enter isbn to search in map");
				String isbn = sc.next();
				Book5 s = map.get(isbn);
				if(s==null)
				System.out.println("isbn not found");
				else
					System.out.println("isbn found");
				break;
			

			}

		} while (choice != 0);

	}

}
